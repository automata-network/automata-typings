{
  "name": "automata-typings",
  "version": "0.3.2",
  "description": "automata type definitions",
  "main": "index.js",
  "scripts": {
    "build": "yarn generate:defs && yarn generate:meta:fst",
    "generate:defs": "ts-node --skip-project node_modules/.bin/polkadot-types-from-defs --package automata-typings/interfaces --input ./src/interfaces",
    "generate:meta:fst": "ts-node --skip-project node_modules/.bin/polkadot-types-from-chain --package automata-typings/interfaces --endpoint ./fst.json --output ./src/interfaces",
    "lint": "tsc --noEmit --pretty"
  },
  "dependencies": {
    "@polkadot/api": "^7.15.1"
  },
  "devDependencies": {
    "@polkadot/typegen": "^7.15.1",
    "@types/node": "^17.0.31",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/automata-network/automata-typings.git"
  },
  "author": "Automata Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/automata-network/automata-typings/issues"
  },
  "homepage": "https://github.com/automata-network/automata-typings#readme"
}
